<?xml version="1.0"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
		<id>SimpleStack.OrmLite.MySql</id>
		<title>OrmLite.MySQL - Fast, code-first, config-free Poco ORM</title>
		<version>1.0.0</version>
		<authors>Demis Bellot, Thomas Grassauer</authors>
		<owners>Demis Bellot, Thomas Grassauer</owners>
		<description>
			Light, simple and fast convention-based code-first POCO ORM for MySQL.
			Support for Creating and Dropping Table Schemas from POCOs, Complex Property types transparently stored in schemaless text blobs in MySQL.
		</description>
		<projectUrl>https://github.com/SimpleStack/SimpleStack.OrmLite</projectUrl>
		<licenseUrl>https://github.com/SimpleStack/SimpleStack/blob/master/LICENSE</licenseUrl>
		<iconUrl>http://www.simplestack.org/logo-100x100.png</iconUrl>
		<tags>MySql OrmLite POCO Code-First Orm Schema-less Blobs</tags>
		<language>en-US</language>
		<copyright>SimpleStack 2014 and contributors</copyright>
		<dependencies>
			<dependency id="MySql.Data" />
			<dependency id="SimpleStack.Common"/>
		</dependencies>
	</metadata>
	<files>
		<file src="lib\**\*.*" target="lib" />
		<file src="..\..\src\SimpleStack.OrmLite\**\*.cs" target="src\SimpleStack.OrmLite" />
		<file src="..\..\src\SimpleStack.OrmLite.MySql\**\*.cs" target="src\SimpleStack.OrmLite.MySql" />
	</files>
</package>
